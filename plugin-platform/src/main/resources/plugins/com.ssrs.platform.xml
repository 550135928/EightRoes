<?xml version="1.0" encoding="UTF-8"?>
<plugin>
    <id>com.ssrs.platform.PlatformPlugin</id>
    <name>framework</name>
    <class>com.ssrs.platform.PlatformPlugin</class>
    <author>ssrs</author>
    <provider>ssrs</provider>
    <version>1.0</version>
    <description>PlatformPlugin</description>
    <extend-action>
        <id>com.ssrs.platform.point.action.AddUserPermissionsAction</id>
        <class>com.ssrs.platform.point.action.AddUserPermissionsAction</class>
        <description>添加用户权限扩展行为</description>
        <extend-point>com.ssrs.framework.point.AddUserPermissionsPoint</extend-point>
    </extend-action>
    <extend-service>
        <id>com.ssrs.platform.extend.MenuPrivService</id>
        <class>com.ssrs.platform.extend.MenuPrivService</class>
        <description>菜单权限扩展服务</description>
        <item-class>com.ssrs.platform.priv.AbstractMenuPriv</item-class>
    </extend-service>
    <extend-service>
        <id>com.ssrs.platform.extend.ConfigService</id>
        <class>com.ssrs.platform.extend.ConfigService</class>
        <description>配置项扩展服务</description>
        <item-class>com.ssrs.platform.FixedConfigItem</item-class>
    </extend-service>
    <extend-service>
        <id>com.ssrs.platform.extend.CodeService</id>
        <class>com.ssrs.platform.extend.CodeService</class>
        <description>代码项扩展服务</description>
        <item-class>com.ssrs.platform.FixedCodeType</item-class>
    </extend-service>
    <extend-item>
        <id>com.ssrs.platform.extend.item.CodeCacheProvider</id>
        <class>com.ssrs.platform.extend.item.CodeCacheProvider</class>
        <description>代码项缓存扩展项</description>
        <extend-service>com.ssrs.framework.cache.CacheService</extend-service>
    </extend-item>
    <extend-item>
        <id>com.ssrs.platform.code.ControlType</id>
        <class>com.ssrs.platform.code.ControlType</class>
        <description>配置项控件类型</description>
        <extend-service>com.ssrs.platform.extend.CodeService</extend-service>
    </extend-item>
    <extend-item>
        <id>com.ssrs.platform.code.DataType</id>
        <class>com.ssrs.platform.code.DataType</class>
        <description>配置项数据类型</description>
        <extend-service>com.ssrs.platform.extend.CodeService</extend-service>
    </extend-item>
    <extend-item>
        <id>com.ssrs.platform.config.AdminUserName</id>
        <class>com.ssrs.platform.config.AdminUserName</class>
        <description>系统管理员的用户名配置项</description>
        <extend-service>com.ssrs.platform.extend.ConfigService</extend-service>
    </extend-item>
</plugin>